<% websites.sort.each do |website, values| -%>
  <%= website %>_ip4:
    prober: http
    http:
      fail_if_not_matches_regexp:
        - "<%= values["regex"] %>"
      preferred_ip_protocol: ip4
  <%= website %>_redirects_ip4 :
    prober: http
    http:
      valid_status_codes: [301]
      method: GET
      no_follow_redirects: true
      preferred_ip_protocol: "ip4"
<% end -%>